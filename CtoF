import java.util.Scanner;
   public class CtoF {
    public static void main ( String [] args ) {
       Scanner input= new Scanner(System.in);
          double f;
          double c;
   
   System.out.println("Enter Celcius Value");
   
     c= input.nextDouble();
   
     f=((9.0/5.0)*c)+32;
     
     System.out.println( "The Farenhight value is: " + f + " degrees");
   }  
}
